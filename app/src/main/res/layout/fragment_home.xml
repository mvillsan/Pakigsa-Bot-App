<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frame_layoutdesign"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_gravity="center"
    android:background="@drawable/homebg_customer"
    android:measureAllChildren="true"
    tools:context=".NavigationFragments.HomeFragment">

    <!-- TODO: Update blank fragment layout -->

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/profileImageView"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/ic_user"
        app:civ_border_color="@color/white"
        app:civ_border_width="2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.142"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.053" />

    <TextView
        android:id="@+id/whatAreULookingTxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_light"
        android:text="@string/whatareulooking"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.084"
        app:layout_constraintStart_toEndOf="@+id/imageView33"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.066" />

    <ImageButton
        android:id="@+id/shareBtn"
        android:layout_width="28dp"
        android:layout_height="44dp"
        android:background="@color/darkgreen"
        android:src="@drawable/ic_share"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.247"
        app:layout_constraintStart_toEndOf="@+id/whatAreULookingTxt"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.048" />

    <ImageButton
        android:id="@+id/signoutHomeBtn"
        android:layout_width="28dp"
        android:layout_height="42dp"
        android:background="@color/darkgreen"
        android:src="@drawable/sign_out"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.171"
        app:layout_constraintStart_toEndOf="@+id/shareBtn"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.047" />

    <ImageButton
        android:id="@+id/restoBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/darkgreen"
        android:src="@drawable/restuarant"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.272"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/whatAreULookingTxt"
        app:layout_constraintVertical_bias="0.037" />

    <TextView
        android:id="@+id/restoTxt"
        android:layout_width="54dp"
        android:layout_height="22dp"
        android:fontFamily="@font/poppins_light"
        android:text="@string/resto"
        android:textAlignment="center"
        android:textColor="@color/lightpurple"
        android:textSize="8sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.26"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/restoBtn"
        app:layout_constraintVertical_bias="0.005" />

    <ImageButton
        android:id="@+id/resortBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/darkgreen"
        android:src="@drawable/resort_icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.085"
        app:layout_constraintStart_toEndOf="@+id/restoBtn"
        app:layout_constraintTop_toBottomOf="@+id/whatAreULookingTxt"
        app:layout_constraintVertical_bias="0.036" />

    <TextView
        android:id="@+id/resortTxt"
        android:layout_width="32dp"
        android:layout_height="13dp"
        android:fontFamily="@font/poppins_light"
        android:text="@string/resort"
        android:textAlignment="center"
        android:textColor="@color/lightpurple"
        android:textSize="8sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.025"
        app:layout_constraintStart_toEndOf="@+id/restoTxt"
        app:layout_constraintTop_toBottomOf="@+id/resortBtn"
        app:layout_constraintVertical_bias="0.006" />

    <ImageButton
        android:id="@+id/dentalClinicBtn"
        android:layout_width="38dp"
        android:layout_height="34dp"
        android:layout_marginStart="5dp"
        android:background="@color/darkgreen"
        android:src="@drawable/clinics_icon_new"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.092"
        app:layout_constraintStart_toEndOf="@+id/resortBtn"
        app:layout_constraintTop_toBottomOf="@+id/whatAreULookingTxt"
        app:layout_constraintVertical_bias="0.03" />

    <TextView
        android:id="@+id/dentalClinicTxt"
        android:layout_width="40dp"
        android:layout_height="12dp"
        android:layout_marginStart="12dp"
        android:fontFamily="@font/poppins_light"
        android:text="@string/dentalc"
        android:textAlignment="center"
        android:textColor="@color/lightpurple"
        android:textSize="8sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.057"
        app:layout_constraintStart_toEndOf="@+id/resortTxt"
        app:layout_constraintTop_toBottomOf="@+id/dentalClinicBtn"
        app:layout_constraintVertical_bias="0.005" />

    <ImageButton
        android:id="@+id/internetCafeBtn"
        android:layout_width="41dp"
        android:layout_height="34dp"
        android:background="@color/darkgreen"
        android:src="@drawable/internetcafe"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.151"
        app:layout_constraintStart_toEndOf="@+id/dentalClinicBtn"
        app:layout_constraintTop_toBottomOf="@+id/whatAreULookingTxt"
        app:layout_constraintVertical_bias="0.036" />

    <TextView
        android:id="@+id/icTxt"
        android:layout_width="58dp"
        android:layout_height="26dp"
        android:fontFamily="@font/poppins_light"
        android:text="@string/ic"
        android:textAlignment="center"
        android:textColor="@color/lightpurple"
        android:textSize="8sp"
        android:layout_marginLeft="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.018"
        app:layout_constraintStart_toEndOf="@+id/dentalClinicTxt"
        app:layout_constraintTop_toBottomOf="@+id/internetCafeBtn"
        app:layout_constraintVertical_bias="0.003" />

    <TextView
        android:id="@+id/spaSalonTxt"
        android:layout_width="53dp"
        android:layout_height="26dp"
        android:fontFamily="@font/poppins_light"
        android:text="@string/spaSalon"
        android:textAlignment="center"
        android:textColor="@color/lightpurple"
        android:textSize="8sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.048"
        app:layout_constraintStart_toEndOf="@+id/icTxt"
        app:layout_constraintTop_toBottomOf="@+id/spaSalonBtn"
        app:layout_constraintVertical_bias="0.01" />

    <ImageButton
        android:id="@+id/spaSalonBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/darkgreen"
        android:src="@drawable/spasalon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toEndOf="@+id/internetCafeBtn"
        app:layout_constraintTop_toBottomOf="@+id/whatAreULookingTxt"
        app:layout_constraintVertical_bias="0.036" />

    <pl.droidsonroids.gif.GifImageView
        android:id="@+id/gifImgView"
        android:layout_width="429dp"
        android:layout_height="314dp"
        android:src="@drawable/reserve_now"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.654"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profileImageView"
        app:layout_constraintVertical_bias="0.374" />

    <TextView
        android:id="@+id/selectRsrvTxt"
        android:layout_width="252dp"
        android:layout_height="47dp"
        android:fontFamily="@font/poppins_light"
        android:text="@string/selectRsrv"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.522"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gifImgView"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView33"
        android:layout_width="16dp"
        android:layout_height="17dp"
        android:src="@drawable/active_status"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.194"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.084" />

</androidx.constraintlayout.widget.ConstraintLayout>