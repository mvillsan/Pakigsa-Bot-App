        app:layout_constraintHorizontal_bias="0.563"
        app:layout_constraintStart_toStartOf="@+id/purple_round"
        app:layout_constraintTop_toTopOf="@+id/purple_round"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/purple_round2"
        android:layout_width="158dp"
        android:layout_height="37dp"
        android:src="@drawable/purple_rect"
